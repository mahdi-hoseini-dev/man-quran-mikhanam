{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      },
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es5",
    "loose": true,
    "externalHelpers": true,
    "keepClassNames": true
  },
  "env": {
    "targets": {
      "browsers": ["> 0.25%", "not dead"]
    },
    "mode": "entry"
  }
}
